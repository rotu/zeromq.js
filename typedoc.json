{
  "$schema": "https://typedoc.org/schema.json",
  "name": "zeromq.js",
  "entryPoints": ["./src/index.ts"],
  "tsconfig": "./tsconfig.docs.json",
  "out": "docs-unminified",
  "excludePrivate": true,
  "excludeExternals": false,
  "exclude": [
    "script/**/*",
    "test/**/*",
    "examples/**/*",
    "build/**/*"
  ],
  "cleanOutputDir": true,
  "pretty": false,
  "githubPages": true
}
